{
  "name": "dna-download_file",
  "version": "1.5.2",
  "author": "David J Felix <felix.davidj@gmail.com>",
  "summary": "A module that will download files for use on Windows servers. Requires the server to have PowerShell 2.0 and above",
  "license": "MIT",
  "source": "https://github.com/hatchery/dna-download_file",
  "project_page": "https://forge.puppetlabs.com/dna/download_file",
  "issues_url": "https://github.com/hatchery/dna-download_file/issues",
  "tags": [
    "windows",
    "utility",
    "download"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "2008R2",
        "2012",
        "2012R2",
        "7",
        "8"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">=3.0.0 <5.0.0"},
    {"name":"puppetlabs/powershell","version_requirement":">= 1.0.1"},
    {"name":"dna/proxy","version_requirement":">= 0.2.0"}
  ]
}
